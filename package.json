{
  "name": "vvvlocal",
  "version": "0.1.0",
  "description": "CLI tool for VVV (Varying Vagrant Vagrants)",
  "module": "src/index.ts",
  "type": "module",
  "bin": {
    "vvvlocal": "./src/index.ts"
  },
  "scripts": {
    "start": "bun run src/index.ts",
    "build": "bun run build:all",
    "build:all": "bun run build:linux-x64 && bun run build:linux-arm64 && bun run build:darwin-x64 && bun run build:darwin-arm64 && bun run build:windows-x64",
    "build:linux-x64": "bun build src/index.ts --compile --target=bun-linux-x64 --outfile=dist/vvvlocal-linux-x64",
    "build:linux-arm64": "bun build src/index.ts --compile --target=bun-linux-arm64 --outfile=dist/vvvlocal-linux-arm64",
    "build:darwin-x64": "bun build src/index.ts --compile --target=bun-darwin-x64 --outfile=dist/vvvlocal-darwin-x64",
    "build:darwin-arm64": "bun build src/index.ts --compile --target=bun-darwin-arm64 --outfile=dist/vvvlocal-darwin-arm64",
    "build:windows-x64": "bun build src/index.ts --compile --target=bun-windows-x64 --outfile=dist/vvvlocal-windows-x64.exe",
    "clean": "rm -rf dist",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "typecheck": "tsc --noEmit",
    "check": "bun run typecheck && bun run lint",
    "test": "bun test",
    "test:unit": "bun test tests/utils tests/commands",
    "test:integration": "VVV_INTEGRATION_TEST=1 bun test tests/integration/integration.test.ts",
    "test:all": "bun test && VVV_INTEGRATION_TEST=1 bun test tests/integration/integration.test.ts",
    "test:watch": "bun test --watch"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/bun": "latest",
    "eslint": "^9.39.2",
    "react-devtools-core": "^7.0.1",
    "typescript-eslint": "^8.50.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@types/react": "^19.2.7",
    "commander": "^14.0.2",
    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "react": "^19.2.3",
    "yaml": "^2.8.2"
  }
}
